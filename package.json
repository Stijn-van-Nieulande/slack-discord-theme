{
  "name": "slack-discord-theme",
  "version": "1.0.0",
  "description": "Discord theme for slack",
  "main": "scripts/setup.js",
  "scripts": {
    "build-sass": "node-sass -r --output-style compressed ./scss/main.scss -o ./out/slack/",
    "watch-sass": "npm run build-sass && npm run build-sass -- -w",
    "start": "node scripts/start.js && npm run watch-sass"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mattco98/slack-discord-theme.git"
  },
  "keywords": [
    "slack",
    "discord",
    "theme",
    "slack-theme",
    "discord-theme",
    "dark",
    "dark-theme",
    "slack-discord-theme",
    "black",
    "black-theme"
  ],
  "author": "Matthew Olsson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mattco98/slack-discord-theme/issues"
  },
  "homepage": "https://github.com/mattco98/slack-discord-theme#readme",
  "dependencies": {
    "node-sass-watcher": "^0.5.1"
  },
  "devDependencies": {
    "chokidar": "^2.0.3",
    "node-sass": "^4.9.0"
  }
}
